{
  "name": "smallconf",
  "version": "1.0.0",
  "description": "organiser tool for conferences and meet-up",
  "main": "Gulpfile.js",
  "scripts": {
    "test": "npm test",
    "start": "node --harmony ./node_modules/pm2/bin/pm2-dev run ./processes.json",
    "lint": "node --harmony ./node_modules/gulp/bin/gulp.js lint",
    "build": "node --harmony ./node_modules/gulp/bin/gulp.js build",
    "release": "node --harmony ./node_modules/gulp/bin/gulp.js release",
    "watch": "node --harmony ./node_modules/gulp/bin/gulp.js watch",
    "watch-lint": "node --harmony ./node_modules/gulp/bin/gulp.js watch-lint",
    "server": "node --harmony lib/server.js",
    "server-release": "NODE_ENV=release node --harmony lib/server.js",
    "start-release": "npm run release && NODE_ENV=release node --harmony lib/server.js"
  },
  "dependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.21",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "better-livereload": "^0.3.1",
    "bootstrap": "^3.3.5",
    "camelcase": "^1.2.1",
    "chalk": "^1.1.0",
    "compression": "^1.5.2",
    "decamelize": "^1.0.0",
    "express": "^4.13.3",
    "extend": "^3.0.0",
    "firebase": "^2.2.9",
    "gulp": "^3.9.0",
    "gulp-css-base64": "^1.3.2",
    "gulp-inline-source": "^2.1.0",
    "gulp-jsxcs": "^0.1.9",
    "gulp-less": "^3.0.3",
    "gulp-lesshint": "^0.2.0",
    "gulp-lesshint-stylish": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "hogan.js": "^3.0.2",
    "less-plugin-clean-css": "^1.5.1",
    "node-notifier": "^4.2.3",
    "pm2": "^0.14.6",
    "react": "^0.13.3",
    "react-bootstrap": "^0.24.3",
    "request": "^2.60.0",
    "webpack": "^1.11.0",
    "webpack-stream": "^2.1.0"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcopeg/SmallConf.git"
  },
  "keywords": [
    "conference",
    "organiser",
    "single",
    "page",
    "application",
    "firebase",
    "reactjs"
  ],
